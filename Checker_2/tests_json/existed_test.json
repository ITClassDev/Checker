{
	"language": "cpp",
    "github_link": "https://github.com/ikoshkila/existed_test_shtp.git",
    "test_type": "header_test",
	"debug": false,
    "tests_description": [
        {
            "env": {
                "mem": 10,
                "proc": 2,
                "time": 2
            },
            "name": "sum",
            "submit_id": "90",
            "tests": [
                {
                    "input": "100\n120",
                    "output": "220"
                },
                {
                    "input": "20\n43",
                    "output": "63"
                },
                {
                    "input": "7\n123",
                    "output": "130"
                }
            ],
            "types": {
                "in": [
                    "int",
                    "int"
                ],
                "out": "int"
            }
        },
        {
            "env": {
                "mem": 10,
                "proc": 2,
                "time": 2
            },
            "name": "str_list",
            "submit_id": "85",
            "tests": [
                {
                    "input": "abcdef",
                    "output": "a b c d e f "
                },
                {
                    "input": "lokira",
                    "output": "l o k i r a "
                },
                {
                    "input": "pgsppl",
                    "output": "p g s p p l "
                }
            ],
            "types": {
                "in": [
                    "string"
                ],
                "out": "vector<char>"
            }
        },
        {
            "env": {
                "mem": 10,
                "proc": 2,
                "time": 2
            },
            "name": "concat",
            "submit_id": "80",
            "tests": [
                {
                    "input": "a\nb",
                    "output": "ab"
                },
                {
                    "input": "Hello,\nworld!",
                    "output": "Hello,world!"
                },
                {
                    "input": "Result\nstring",
                    "output": "Resultstring"
                }
            ],
            "types": {
                "in": [
                    "string",
                    "string"
                ],
                "out": "string"
            }
        }
    ]
}
